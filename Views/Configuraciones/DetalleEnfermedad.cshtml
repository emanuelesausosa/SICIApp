@model SICIApp.Models.CONDICIONFISICA_ENFERMEDADESMODEL

@{
    ViewBag.Title = "Detalle Enfermedad";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>Detalle Enfermedad</h2>
        <ol class="breadcrumb">
            <li>
                @Html.ActionLink("Lista", "Enfermedades")
            </li>
            <li class="active">
                <strong>Detalle Enfermedad</strong>
            </li>
        </ol>
    </div>
</div>




<div class="wrapper wrapper-content animated fadeInRight">
