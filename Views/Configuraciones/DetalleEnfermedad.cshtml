        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Detalle Enfermedad</h5>
                </div>
                <div class="ibox-content">


				    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.NOMBRECIENTIFICO)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.NOMBRECIENTIFICO)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.NOMBRESINOMIMO)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.NOMBRESINOMIMO)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.DESCRIPCION)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.DESCRIPCION)
        </dd>

    </dl>


	    @Html.ActionLink("Editar", "EditarEnfermedad", new { ID = Model.ID }, new { @class = "btn btn-primary" })
    @Html.ActionLink("Todas", "Enfermedades", null, new { @class = "btn btn-white" })

                </div>
            </div>
        </div>
    </div>
 </div>













