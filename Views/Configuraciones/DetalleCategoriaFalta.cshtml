@model SICIApp.Models.TER_CATEGORIAFALTAMODEL

@{
    ViewBag.Title = "Detalle Categoria Falta";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>Detalle Categoria Falta</h2>
        <ol class="breadcrumb">
            <li>
                @Html.ActionLink("Lista", "CategoriasFaltas")
            </li>
            <li class="active">
                <strong>Detalle Categoria Falta</strong>
            </li>
        </ol>
    </div>
</div>




<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Detalle Categoria Falta</h5>
                </div>
                <div class="ibox-content">


				    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.NOMBRE)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.NOMBRE)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.DESCRIPCION)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.DESCRIPCION)
        </dd>

    </dl>


	    @Html.ActionLink("Editar", "EditarCategoriaFalta", new { ID = Model.ID }, new { @class = "btn btn-primary" })
    @Html.ActionLink("Todos", "CategoriasFaltas", null, new { @class = "btn btn-white"})

                </div>
            </div>
        </div>
    </div>
 </div>













