@model IEnumerable<SICIApp.Models.EXAMENPSICOMETRICO_EXAMENCONTENIDOMODEL>

@{
    ViewBag.Title = "Contenidos Exámenes";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>Contenidos Exámenes</h2>
        <ol class="breadcrumb">
            <li>
                @Html.ActionLink("Configuraciones","Index");
            </li>
            <li class="active">
                <strong>EXAMEN PSICOMETRICO CONTENIDO</strong>
            </li>
        </ol>
    </div>
</div>


<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Lista de EXAMEN PSICOMETRICO CONTENIDO</h5>
					<div class="ibox-tools">
						@Html.ActionLink("Crear nuevo", "NuevoContenidoExamen", null, new { @class = "btn btn-primary btn-xs" })
					</div>
                </div>
                <div class="ibox-content">

<table class="table table-striped">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DETALLE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VALORACION)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DESCRIPCION)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IDEXAMEN)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DETALLE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VALORACION)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DESCRIPCION)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IDEXAMEN)
        </td>
        <td>
			@Html.ActionLink("Detallles", "DetalleContenidoExamen", new { ID = item.IDPREGUNTA }, new { @class = "btn btn-primary btn-sm" })
            @Html.ActionLink("Editar", "EditarContenidoExamen", new { ID = item.IDPREGUNTA }, new { @class = "btn btn-white btn-sm" })            
        </td>
    </tr>
}

</table>

                </div>
            </div>
        </div>
    </div>
 </div>



